# YouTube Data API v3 - Chave de Desenvolvedor
# Para obter uma chave: https://console.developers.google.com/apis/credentials
YOUTUBE_API_KEY=your_youtube_api_key_here

# Configurações da API
YOUTUBE_API_QUOTA_LIMIT=10000
YOUTUBE_API_QUOTA_RESET=24h

# Configurações de transcrição
TRANSCRIPT_LANGUAGE=pt-BR
TRANSCRIPT_FALLBACK_LANGUAGE=en

# Configurações de IA
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
AI_MODEL=gpt-3.5-turbo
AI_MAX_TOKENS=1000

# Configurações de Transcrição de Áudio
ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here
AUDIO_QUALITY=high
AUDIO_FORMAT=mp3
MAX_AUDIO_DURATION=3600

# Configurações do yt-dlp
YTDLP_PATH=yt-dlp
YTDLP_AUDIO_QUALITY=0
YTDLP_AUDIO_FORMAT=mp3

# Configurações do aplicativo
APP_NAME=YouTube Live Summarizer
APP_VERSION=2.0.0
APP_ENV=development



